  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "dts-bundle-generator": "^8.0.1",
    "bun-types": "^1.0.3",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "type": "module",
  "exports": {
    "./hls-parser": {
      "types": "./dist/hls-parser/index.d.ts",
